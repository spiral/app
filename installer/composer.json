{
    "name": "spiral/app",
    "license": "MIT",
    "description": "Spiral Application installer",
    "require-dev": {
        "composer/composer": "^2.4",
        "mockery/mockery": "^1.5",
        "nette/php-generator": "^4.1.2",
        "phpunit/phpunit": "^10.1",
        "spiral/dumper": "^3.3",
        "spiral/framework": "^3.15.7",
        "spiral/nyholm-bridge": "^1.3",
        "vimeo/psalm": "^6.10"
    },
    "autoload": {
        "psr-4": {
            "Installer\\Application\\": "Application",
            "Installer\\Internal\\": "Internal",
            "Installer\\Module\\": "Module"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "spiral/composer-publish-plugin": true
        }
    }
}
